{
	"appId": "uk.ac.imperial.epicollect.five",
	"appName": "Epicollect5",
	"npmClient": "npm",
	"webDir": "dist",
	"plugins": {
		"StatusBar": {
			"style": "DARK"
		},
		"SplashScreen": {
			"androidScaleType": "CENTER_CROP",
			"launchAutoHide": false,
			"splashFullScreen": true,
			"splashImmersive": false,
			"backgroundColor": "#ffffff"
		},
		"EdgeToEdge": {
			"backgroundColor": "#5b357f"
		}
	},
	"cordova": {
		"preferences": {
			"webviewbounce": "false",
			"DisallowOverscroll": "true",
			"android-minSdkVersion": "29",
			"android-targetSdkVersion": "33",
			"BackupWebStorage": "none",
			"ShowSplashScreenSpinner": "false",
			"AutoHideSplashScreen": "false",
			"FadeSplashScreen": "false",
			"FadeSplashScreenDuration": "10",
			"AndroidPersistentFileLocation": "Compatibility",
			"cordova.plugins.diagnostic.modules": "LOCATION WIFI CAMERA NOTIFICATIONS MICROPHONE EXTERNAL_STORAGE"
		}
	},
	"packageClassList": [
		"ActionSheetPlugin",
		"AppPlugin",
		"CAPCameraPlugin",
		"DevicePlugin",
		"DialogPlugin",
		"FilesystemPlugin",
		"GeolocationPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"LocalNotificationsPlugin",
		"CAPNetworkPlugin",
		"PushNotificationsPlugin",
		"SharePlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"ToastPlugin",
		"CDVPlugin"
	]
}
